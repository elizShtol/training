  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.2"
  }
}
